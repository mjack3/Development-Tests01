<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
	
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.StudentToStringConverter" />
		<bean class="converters.TeacherToStringConverter" />
		<bean class="converters.ActivityRecordToStringConverter" />
		<bean class="converters.FolderToStringConverter" />
		<bean class="converters.SeminarToStringConverter" />
		<bean class="converters.SocialIdentityToStringConverter" />
		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.SubmissionToStringConverter"/>
		<bean class="converters.SubjectToStringConverter" />
		
		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.StringToStudentConverter" />
		<bean class="converters.StringToTeacherConverter" />
		<bean class="converters.StringToActivityRecordConverter" />
		<bean class="converters.StringToFolderConverter" />
		<bean class="converters.StringToSeminarConverter" />
		<bean class="converters.StringToSocialIdentityConverter" />
		<bean class="converters.StringToAuthorityConverter" />
		<bean class="converters.StringToSubmissionConverter" />
		<bean class="converters.StringToSubjectConverter" />
		
		
		<!-- KARLI -->
		
		<bean class="converters.GroupSubjectToStringConverter" />  
  		<bean class="converters.StringToGroupSubjectConverter" /> 
		
		
	</util:list>
	
</beans>